        "json": "{ \"must\": [ { \"match\": { \"last_name.phonetic\": { \"query\": \"{VALUE}\", \"boost\": 10, \"fuzziness\": 4 } } } ] }",
        "value_path": "match;lastname.phonetic;query"
      }
    ]
  },
